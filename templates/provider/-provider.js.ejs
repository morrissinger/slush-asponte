 */
(function () {

	class Service {
		constructor(<%= injections.formatted %>) {}<% if (methods.list.length > 0) { %>
		<% _.each(methods.list, function (method) { %>
		/**
		 * <%= method %> method.
		 */
		<%= method %>() {
		}
		<% }) %> <% } %>
